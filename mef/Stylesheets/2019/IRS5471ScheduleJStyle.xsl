<?xml version="1.0" encoding="UTF-8" ?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">


<!-- CSS Styles for form 5471ScheduleJ -->


<xsl:template name="IRS5471ScheduleJStyle">
    .sty5471ScheduleJFNum2 {   /* Form Number 2nd Line */
    font-family: verdana, arial, sans-serif;
    font-size: 10pt; font-weight: bold;
  }
  .sty5471ScheduleJRev {
    font-size: 7pt; font-weight: normal;
  }
 .sty5471ScheduleJBusinessName {   /* Business Name */
    font-size: 7pt;
    padding-top: 0px; padding-bottom: 0px; padding-left: 0px; padding-right: 0px;
    border-style: solid; border-color: black;
    border-top-width: 0px; border-bottom-width: 0px; border-left-width: 0px; border-right-width: 1px;
    float: left; clear: none;
  }
 .sty5471ScheduleJIN {   /*IN */
    font-size: 7pt;
    text-align: center;
    padding-top: 0px; padding-bottom: 0px; padding-left: 0px; padding-right: 0px;
    border-style: solid; border-color: black;
    border-top-width: 0px; border-bottom-width: 0px; border-left-width: 0px; border-right-width: 0px;
    float: left; clear: none;
  }
  .sty5471ScheduleJINBox{ /*Employer ID number box*/
    font-size:7pt;
    font-weight: bold;
    float: left; clear: none;
  }
  .sty5471ScheduleJCorporation {
    width: 256mm;
    border-style: solid; border-color: black;
    border-top-width: 1px; border-bottom-width: 0px; border-left-width: 0px; border-right-width: 0px;
  }
  .sty5471ScheduleJLNCol {
    height: 8mm;
    width: 62mm;
    font-size: 7pt;
    border: 1px solid;
    border-left: 0; border-top: 0;
    text-align: left;
   }
 .sty5471ScheduleJCellGeneric {
    height: 8mm;
    width:32mm;
    font-size: 7pt;
    font-weight:normal;
    border: 1px solid black;
    border-left: 0; border-top: 0;
    text-align: right;
    }
 .sty5471ScheduleJDescLine {   /* Desc Line */
    font-size: 7pt;
    width: 256mm;
  }
  .sty5471ScheduleJLN {   /* Line Nubmer */
    color: black;
    font-weight: bold;
    text-align: right;
    float: left;
  }
  .sty5471ScheduleJLine {
    font-size: 7pt;
    color: black;
    margin-left: 2mm; margin-right: 0px; margin-top: 0px; margin-bottom: 0px;
    float: left;
  }
  .sty5471ScheduleJFormFooter {   /* Form Footer */
    width:256mm;
    font-size: 7pt;
    text-align: left;
  }
</xsl:template>

</xsl:stylesheet>