<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<!--  CSS Styles for form W2G  -->
	<xsl:template name="IRSW2GStyle">
	.styIRSW2GLeftTopFrame {
		width:155mm;
		height:71mm;
		border-top-width:1px;
		border-bottom-width: 1px;
		border-left-width:1px;
		border-right-width:1px;
		border-style:solid;
		border-color:#000000;
		float:left;
		padding: 0px 0px 0px 0px;
	}
	.styIRSW2GLeftBottomFrame {
		width:187mm;
		height:15mm;
		border-top-width:0px;
		border-bottom-width: 1px;
		border-left-width:1px;
		border-right-width:0px;
		border-style:solid;
		border-color:#000000;
		float:left;
		padding: 0px 0px 0px 0px;
	}
    .styIRSW2GRightTopFrame {
		width:32mm;
		height:71mm;
		border-top-width:0px;
		border-bottom-width: 1px;
		border-left-width:0px;
		border-right-width:0px;
		border-style:solid;
		border-color:#000000;
		float:right;
		padding: 0px 0px 0px 0px;
	}
	.styIRSW2GRightBottomFrame {
		width:32mm;
		height:15mm;
		border-top-width:0px;
		border-bottom-width: 1px;
		border-left-width:0px;
		border-right-width:0px;
		border-style:solid;
		border-color:#000000;
		float:left;
		padding: 0px 0px 0px 0px;
	}
	.styIRSW2GHeaderFrame {
		width:100%;
		height:4mm;
		border-top-width:0px;
		border-bottom-width: 0px;
		border-left-width:0px;
		border-right-width:0px;
		border-style:solid;
		border-color:#000000;
		padding: 0px 0px 0px 0px;
	}
	.styIRSW2GFooterFrame {
		width:100%;
		height:8mm;
		border-top-width:0px;
		border-bottom-width: 0px;
		border-left-width:0px;
		border-right-width:0px;
		border-style:solid;
		border-color:#000000;
		padding: 0px 0px 0px 0px;
	}
	.styIRSW2GDataTitleLeft {
		border-top-width:0px;
		border-bottom-width: 0px;
		border-left-width:0px;
		border-right-width:0px;
		width:79mm;
		padding:2px 0px 0px 3px;
		text-align:left;
		border-style: solid; 
		border-color: black;
		float:left; 
		clear: none;
		height:4mm;
		font-size:7pt;
	}
	.styIRSW2GDataMRTitle {
		border-top-width:0px;
		border-bottom-width: 0px;
		border-left-width:1px;
		border-right-width:0px;
		width:37mm;
		padding:2px 0px 0px 3px;
		text-align:left;
		border-style: solid; 
		border-color: black;
		float:left; 
		clear: none;
		height:4mm;
		font-size:6pt;
	}
	.styIRSW2GDataMRTxtFields {
		border-top-width:0px;
		border-bottom-width: 1px;
		border-left-width:1px;
		border-right-width:0px;
		width:37.7mm;
		padding:5px 0px 0px 5px;
		text-align:left;
		border-style: solid; 
		border-color: black;
		float:left; 
		clear: none;
		font-size:6pt;
		height:6mm;
	}
	.styIRSW2GDataLTxtField {
		border-top-width:0px;
		border-bottom-width: 0px;
		border-left-width:0px;
		border-right-width:0px;
		width:79mm;
		padding:0px 0px 0px 3px;
		text-align:left;
		border-style: solid; 
		border-color: black;
		float:left; 
		clear: none;
		text-align:left;
		font-size:6pt;
		font-family:Arial;
		height:6mm;
	}
	.styIRSW2GRowItem {
		border-top-width:0px;
		border-bottom-width: 0px;
		border-left-width:0px;
		border-right-width:0px;
		width:100%mm;
		height:4mm;
		padding:0px 0px 0px 0px;
		text-align:left;
		float:left; 
		clear: none;
	}
	.styIRSW2GLColumn {
		width:83mm;
		float:left;
		clear:none;
		border-top-width:0px;
		border-bottom-width: 0px;
		border-left-width:0px;
		border-right-width:0px;
		padding:0px 0px 0px 0px;
		height:100%;
	}
	.styIRSW2GMRColumn {
		width:35mm;
		float:left;
		clear:none;
		border-top-width:0px;
		border-bottom-width: 0px;
		border-left-width:1px;
		border-right-width:0px;
		padding:0px 0px 0px 0px;
		border-style: solid; 
		border-color: black;
		height:100%;
	}
	
	.styIRSW2GCell {
	    border-top:1px solid black;
	    border-right:1px solid black;
	}
	.wordwrap{/* this is to make lines wrap */	
	  display:block;
	  width:41mm;
	  word-wrap:break-word;            }
	
  </xsl:template>
</xsl:stylesheet>
