<?xml version="1.0"?>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:template name="IRS3115Style">          
    
    .styIRS3115SchAP1 {
      height:4.7mm;    
      width:33mm;      
      font-size:8pt;
      text-align:right;      
      border-bottom:1 solid black;
      border-left:1 solid black;      
      float:left;         
    }
      
    .styIRS3115SchDP3 {  
      height:4.5mm;              
      width:23mm;      
      font-size:7pt;      
      border-bottom:1 solid black;
      border-left:1 solid black;      
      float:left;   
      padding-top:0.5mm;
      text-align:center      
    }
      
    .styIRS3115TableContainer {   
      width:181mm;      
      overflow-y:auto;
      clear:both;    
      margin-left:6mm;      
    }  
    
    .styIRS3115TableContainer2 {   
      width:187mm;
      overflow-y:auto;
      clear:both;          
    }    
      
    .styIRS3115YesBox {    
      height:4mm;    
      width:7mm;      
      text-align:center;
      border-right:1 solid black;
      border-left:1 solid black;      
      float:left;   
      border-bottom:1 solid black;  
      font-size:7.5pt      
    }    
    
    .styIRS3115NoBox {            
      width:7mm;    
      height:4mm;        
      text-align:center;        
      float:left; 
      border-bottom:1 solid black;    
      font-size:7.5pt      
    }  
    
    .styIRS3115DimYesBox {    
      height:4mm;    
      width:7mm;      
      background-color:lightgrey;
      border-right:1 solid black;
      border-left:1 solid black;      
      float:left;                 
    }  
        
    .styIRS3115DimNoBox {  
      height:4mm;            
      width:7mm;      
      background-color:lightgrey;              
      float:left;             
    }                      
    
    .styIRS3115LNLeftLtrBox {         
      width:6mm;        
      font-weight:bold;
      float:left;        
      text-align:right;
      padding-right:1mm;        
      clear:left
    }
    
    
    .styIRS3115LNLeftNumBox {          
      width:6mm;
      font-weight:bold;
      float:left;     
      text-align:left;  
      clear:left;    
    }                    
          
    .styIRS3115TblRB {
      border-bottom:1 solid black;
      border-right:1 solid black;                  
    }  
    
    .styIRS3115TblRB2 {
      border-bottom:1 solid black;
      border-left:1 solid black;    
      font-size:7pt; 
      height:4.5mm    
    }            
    
    .styIRS3115YNChxBoxDiv { 
      float:right; 
      padding-right:1.5mm
    }              
          
    .styIRS3115ChxBoxDiv { 
      float:left;
      margin-right:1mm;     
      clear:left      
    }              
      
  </xsl:template>
</xsl:stylesheet>