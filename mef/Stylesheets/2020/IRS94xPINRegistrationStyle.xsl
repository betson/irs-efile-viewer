<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  <!-- CSS Styles for form 94x pin -->
  <xsl:template name="IRS94xPINRegistrationStyle">
	body {
		font-family:verdana,arial,sans-serif;
		font-size:13px;
	}
	td {
		padding: 0px 0px 0px 0px;
		background-color:#d6ecee;
		
	}
	.label {
		font-weight:bold;
		width:300px;
	}
	.wrapable {
		max-width:530px;
		word-wrap:break-word;
	}
	.formattedTable {
		border-spacing:2px;
		width:740px;
		margin-left:5px;
		margin-right:5px;
	}



</xsl:template>
</xsl:stylesheet>
