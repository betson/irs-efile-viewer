<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<!-- CSS Styles for form 940ScheduleA -->
	<xsl:template name="IRS940ScheduleAStyle">
   
	.styIRS940SATableRow {
	   width:350px;
   }
   
   .styIRS940SACkbox {   /* Checkbox */
	height: 4.5mm; width: 4.5mm;
   }

   .styIRS940SAHeaderPA {
		width:60px;height:8.2mm;
		padding-top:.5mm;
		padding-bottom:.5mm;
		text-align:center;
		float:left;clear:none;
		font-weight:bold;
		font-size:6pt;
		border-style: solid;
		border-color: black;
		border-width: 0px 0px 1px 0px;
		background-color:lightgrey;
   }
   
   .styIRS940SAHeaderLong {
	   width:114px;height:8.2mm;
	   padding-top:.5mm;
		padding-bottom:.5mm;
		text-align:center;
	   float:left;clear:none;
	   font-weight:bold;
	   border-style: solid;
	   border-color: black;
	   border-width: 0px 0px 1px 1px;
	   background-color:lightgrey;
   }
   
   .styIRS940SAHeaderRR {
	   width:62px;height:8.2mm;
	   padding-top:.5mm;
		padding-bottom:.5mm;
		text-align:center;
	   float:left;clear:none;
	   font-weight:bold;
	   border-style: solid;
	   border-color: black;
	   border-width: 0px 0px 1px 1px;
	   background-color:lightgrey;
   }
   
   .styIRS940SAAmtBoxPA {
	   width:30px;height:6.2mm;
		text-align:left;
		padding-top:0mm;
		float:left;clear:none;
		border-style: solid;
		border-color: black;
		border-width: 0px;
		font-family:Courier New;font-size:10pt;
   }
   
   .styIRS940SAAmtBoxLong {
	   width:114px;height:6.2mm;
		text-align:center;
		padding-top:3mm;
		padding-right:1mm;
		text-align:right;
	   float:left;clear:none;
	   font-size:6pt;
	   border-style: solid;
	   border-color: black;
	   border-width: 0px 0px 1px 1px;
   }
   
   .styIRS940SAAmtBoxRR {
	   width:62px;height:6.2mm;
	   text-align:center;
	   padding-top:1.6mm;
	   float:left;clear:none;
	   font-size:8pt;
	   border-style: solid;
	   border-color: black;
	   border-width: 0px 0px 1px 1px;
   }
   
</xsl:template>
</xsl:stylesheet>
